{
  "title": "Real-time tracking of influenza B/Vic evolution using data from GISAID",
  "maintainers": [
    {"name": "Jover Lee", "url": "https://bedford.io/team/jover-lee/"},
    {"name": "Richard Neher", "url": "https://neherlab.org/richard-neher.html"},
    {"name": "Trevor Bedford", "url": "https://bedford.io/team/trevor-bedford/"}
  ],
  "build_url": "https://github.com/nextstrain/seasonal-flu",
  "colorings": [
    {
      "key": "gt",
      "title": "Genotype",
      "type": "categorical"
    },
    {
      "key": "num_date",
      "title": "Date",
      "type": "continuous"
    },
    {
      "key": "clade_membership",
      "title": "Clade",
      "type": "categorical"
    },
    {
      "key": "cTiter",
      "title": "Antigenic advance (tree model)",
      "type": "continuous"
    },
    {
      "key": "cTiterSub",
      "title": "Antigenic advance (sub model)",
      "type": "continuous"
    },
    {
      "key": "cTiter_x",
      "title": "HI antigenic novelty",
      "type": "continuous"
    },
    {
      "key": "lbi",
      "title": "Local branching index",
      "type": "continuous"
    },
    {
      "key": "region",
      "title": "Region",
      "type": "categorical"
    },
    {
      "key": "country",
      "title": "Country",
      "type": "categorical"
    },
    {
      "key": "submitting_lab",
      "title": "Submitting lab",
      "type": "categorical"
    },
    {
      "key": "originating_lab",
      "title": "Originating lab",
      "type": "categorical"
    }
  ],
  "geo_resolutions": [
    "country",
    "region"
  ],
  "display_defaults": {
    "map_triplicate": true,
    "color_by": "clade_membership"
  },
  "filters": [
    "clade_membership",
    "region",
    "country",
    "submitting_lab"
  ],
  "panels": [
    "tree",
    "map",
    "entropy",
    "frequencies"
  ],
  "vaccine_choices": {
    "B/Malaysia/2506/2004": "2006-09-25",
    "B/Brisbane/60/2008": "2009-09-25",
    "B/Colorado/6/2017": "2018-02-22",
    "B/Washington/2/2019": "2019-09-27"
  }
}
